Host *
  ServerAliveInterval 30
  ServerAliveCountMax 10
  IgnoreUnknown UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile ~/.ssh/known_hosts

Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519

Host gitee.com *.coding.net
  User git
  IdentityFile ~/.ssh/id_rsa_yoi
